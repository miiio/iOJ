<android.support.v7.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:card_view="http://schemas.android.com/tools"
    android:id="@+id/cardView"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="-1dp"
    android:layout_marginRight="-1dp"
    android:layout_marginTop="8dp"
    android:layout_marginBottom="7dp"
    android:clickable="true"

    android:foreground="?android:attr/selectableItemBackground"
    app:cardCornerRadius="0dp"
    app:cardElevation="1dp"
    android:contextClickable="false">
    <LinearLayout
        android:id="@+id/status_backcolor"
        android:orientation="vertical"
        android:paddingBottom="10dp"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingRight="12dp"
            android:paddingTop="5dp"
            android:paddingLeft="12dp">
            <TextView
                android:id="@+id/status_problemid"
                android:layout_width="0dp"
                android:layout_weight="1"
                android:layout_height="wrap_content"
                android:textColor="@color/secondary_text"
                android:text="Problem ID:1187"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="5120160446"
                android:textStyle="bold"
                android:textColor="@color/secondary_text"
                android:id="@+id/status_username" />

        </LinearLayout>
        <ImageView
            android:layout_marginTop="3dp"
            android:layout_marginLeft="11dp"
            android:layout_marginRight="11dp"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:src="@drawable/line"/>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_width="wrap_content"
                android:orientation="vertical"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="20dp"
                android:paddingTop="1dp"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:paddingBottom="5dp"
                android:layout_height="match_parent">
                <ImageView
                    android:id="@+id/status_ic_result"
                    android:layout_width="50dp"
                    android:layout_weight="1"
                    android:layout_height="50dp"
                    android:src="@drawable/waitting"/>
                <TextView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:textColor="@color/secondary_text"
                    android:textSize="11sp"
                    android:textStyle="bold"
                    android:text="WA"
                    android:id="@+id/status_result" />
            </LinearLayout>
            <ImageView
                android:layout_marginLeft="15dp"
                android:layout_marginTop="10dp"
                android:layout_marginBottom="2dp"
                android:layout_height="match_parent"
                android:layout_width="1dp"
                android:background="#e5e5e5"/>
            <LinearLayout
                android:orientation="vertical"
                android:paddingTop="18dp"
                android:paddingLeft="13dp"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <LinearLayout
                    android:paddingBottom="5dp"
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:textColor="@color/primary_text"
                        android:text="Memory[KB]："/>
                    <TextView
                        android:id="@+id/stauts_memory"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:text="1132"/>
                </LinearLayout>
                <LinearLayout
                    android:paddingBottom="5dp"
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:textColor="@color/primary_text"
                        android:text="Time[MS]："/>
                    <TextView
                        android:id="@+id/status_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:text="51"/>
                </LinearLayout>
                <LinearLayout
                    android:paddingBottom="5dp"
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:textColor="@color/primary_text"
                        android:text="Lenght[B]："/>
                    <TextView
                        android:id="@+id/stauts_lenght"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textSize="16sp"
                        android:text="764"/>
                </LinearLayout>


            </LinearLayout>
            <TextView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:id="@+id/status_submittime"
                android:gravity="right|bottom"
                android:textSize="12sp"
                android:paddingRight="12dp"
                android:textColor="@color/secondary_text"
                android:text="32s ago"/>
        </LinearLayout>
    </LinearLayout>
</android.support.v7.widget.CardView>